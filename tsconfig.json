{
  "compilerOptions": {
    "outDir": "lib",
    "declaration": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "stripInternal": true,
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src"]
}
